  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "*",
    "phpmd/phpmd": "2.*"
  },
  "scripts" :{
    "test": [
      "php vendor/bin/phpcs -p -s -v -n . --standard=./codesniffer.ruleset.xml --extensions=php",
      "vendor/bin/phpmd ./ text phpmd.ruleset.xml --suffixes php --exclude /vendor/"
    ]
  },
  "autoload": {
    "psr-4": {
      "Mimizuku\\": ""
    }
  }
}
